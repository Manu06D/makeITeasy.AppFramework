<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Nullable>enable</Nullable>
		<Authors>Emmanuel D.</Authors>
		<Company>Make IT Easy</Company>
		<Description>Infrastructure module for makeITeasy.Appframework</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/Manu06D/makeITeasy.AppFramework</RepositoryUrl>
		<PackageTags>framework ef</PackageTags>
		<Version>1.1.21</Version>
		<PackageReleaseNotes>
			1.1.21 : Dependencies update
			1.1.20 : update dependencies
			1.1.19 : Fix issue with LastModificationDate
