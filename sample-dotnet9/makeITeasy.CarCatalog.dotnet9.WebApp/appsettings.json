{
  "ConnectionStrings": {
    "dbConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=CarCatalog;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "DatabaseOptions": {
    "CommandTimeOut": 30,
    "EnableSensitiveDataLogging": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "System": "Warning",
        "Ubaldi.AspNetCore.Security.BasicToken.TokenBasicAuthenticationHandler": "Warning",
        "Ubaldi.DomainLogistics.LogisticsCoreApp.Infrastructure.Export.BaseExportFtp": "Information"

      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug" ],
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "SampleApp"
    }
  },
  "AllowedHosts": "*"
}
